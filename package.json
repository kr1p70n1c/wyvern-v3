{
  "name": "wyvern-v3",
  "version": "3.1.0",
  "description": "Wyvern Protocol v3.1, Ethereum implementation",
  "main": "index.js",
  "scripts": {
    "analyze": "scripts/analyze.sh",
    "flatten": "scripts/flatten.sh",
    "compile": "scripts/compile.sh",
    "compile-hardhat": "hardhat clean && hardhat compile",
    "coverage": "scripts/coverage.sh",
    "test": "scripts/test.sh",
    "lint": "scripts/lint.sh",
    "testrpc": "scripts/testrpc.sh",
    "hardhat:clear-abi": "hardhat clear-abi",
    "hardhat:export-abi": "hardhat export-abi",
    "hardhat:re-export-abi": "npm run hardhat:clear-abi && npm run hardhat:export-abi",
    "hardhat:test": "npm run hardhat:re-export-abi && hardhat test",
    "deploy:testnet-rinkarby:migrations": "NETWORK=rinkarby hardhat run --network rinkarby scripts/deploy-migrations.ts",
    "deploy:testnet-rinkarby:wyvern-atomicizer": "NETWORK=rinkarby hardhat run --network rinkarby scripts/deploy-wyvern-atomicizer.ts",
    "deploy:testnet-rinkarby:wyvern-static": "NETWORK=rinkarby hardhat run --network rinkarby scripts/deploy-wyvern-static.ts",
    "deploy:testnet-rinkarby:static-market": "NETWORK=rinkarby hardhat run --network rinkarby scripts/deploy-static-market.ts",
    "deploy:testnet-rinkarby:wyvern-registry": "NETWORK=rinkarby hardhat run --network rinkarby scripts/deploy-wyvern-registry.ts",
    "deploy:testnet-rinkarby:wyvern-exchange": "NETWORK=rinkarby hardhat run --network rinkarby scripts/deploy-wyvern-exchange.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wyvernprotocol/wyvern-v3.git"
  },
  "author": "Wyvern Protocol Developers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wyvernprotocol/wyvern-v3/issues"
  },
  "homepage": "https://github.com/wyvernprotocol/wyvern-v3#readme",
  "devDependencies": {
    "@babel/runtime": "^7.12.5",
    "@metamask/eth-sig-util": "^4.0.1",
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "coveralls": "^3.1.0",
    "dotenv": "^8.2.0",
    "ethereumjs-testrpc-sc": "^6.5.1-sc.1",
    "ethers": "^5.6.8",
    "ganache-cli": "^6.12.1",
    "ganache-core": "^2.13.1",
    "hardhat": "^2.9.9",
    "hardhat-abi-exporter": "^2.9.0",
    "hardhat-gas-reporter": "^1.0.8",
    "kriptou.js": "0.0.37",
    "lodash": "^4.17.20",
    "openzeppelin-solidity": "^3.3.0",
    "prettier": "^2.6.2",
    "solidity-coverage": "^0.7.12",
    "solidity-docgen": "^0.6.0-beta.16",
    "solium": "^1.2.5",
    "standard": "^16.0.3",
    "truffle": "^5.1.55",
    "truffle-flattener": "^1.5.0",
    "truffle-hdwallet-provider": "^1.0.17",
    "truffle-hdwallet-provider-klaytn": "^1.0.13-a",
    "ts-node": "^10.8.1",
    "typechain": "^8.0.0",
    "typescript": "^4.7.3",
    "web3": "^1.3.0"
  },
  "dependencies": {}
}
